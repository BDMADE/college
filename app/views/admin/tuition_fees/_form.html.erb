  <div class="field">
    <%= f.label :admin_admission_fees_status %><br/>
    <%= f.collection_select :admin_admission_fees_status_ids, Admin::AdmissionFeesStatus.all, :id, :name, {:prompt => true}, {multiple: true} %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
